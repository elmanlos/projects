<h2>JavaScript Objects</h2>

<p id="demo"></p>
<p id="demu"></p>
<script>

let vehicles = {type:"Car", brand:"Toyota", color:"Red"};
let vehicles2 = {type:"Car", brand:"Toyota", color:"Yellow"};

document.getElementById("demo").innerHTML = "The car type is a " + vehicles.type + ",  with brand " + vehicles.brand + " and color " + vehicles.color;
document.getElementById("demu").innerHTML = "The car type is a " + vehicles2.type + ",  with brand " + vehicles2.brand + " and color " + vehicles2.color;
</script>

